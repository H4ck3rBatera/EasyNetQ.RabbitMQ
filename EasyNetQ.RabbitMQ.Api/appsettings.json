{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RabbitMQ": "amqp://messaging:containers@localhost"
  },
  "Exchanges": {
    "ExchangeKey": "RabbitMQ-Exchange"
  },
  "VirtualHosts": {
    "VirtualHostKey": "RabbitMQ-VirtualHost"
  },
  "Queues": {
    "QueueKey": "RabbitMQ-Queue"
  }
}
